{
    "name": "devhun/delynet-api-client",
    "description": "PHP API client for Delynet WEBFAX",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "devhun",
            "email": "i@huns.kr"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "guzzlehttp/guzzle": "^6.4"
    },
    "suggest": {
        "guzzlehttp/guzzle": "To use GuzzleHttpAdapter, require guzzlehttp/guzzle:^6.4"
    },
    "autoload": {
        "psr-4": {
            "DevHun\\Delynet\\": "src"
        }
    }
}
